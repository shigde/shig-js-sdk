    "@angular/router": "^21.0.6",
    "@material/web": "^2.3.0 ",
    "@types/debug": "^4.1.12",
    "dexie": "^3.2.4",
    "rxjs": "~7.8.0",
    "sdp-transform": "^2.14.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "debug": "^4.4.3"
  },
